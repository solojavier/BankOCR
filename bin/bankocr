#!/usr/bin/env ruby

require 'thor'
require 'bankocr'

module BankOCR
  class ProcessCommand < Thor
    desc 'process input_path output_path',
      'Generates a report for account numbers read from input'

    def process(input, output)
      BankOCR.process(input, output)
    end
  end
end

BankOCR::ProcessCommand.start
